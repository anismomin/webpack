{
  "name": "webpackbasic",
  "version": "1.0.0",
  "description": "basic webpack",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot --port 3000",
    "build:prod": "webpack -p && cp src/index.html dist/index.html"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Anis Momin",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "css-loader": "^0.25.0",
    "style-loader": "^0.13.1",
    "webpack-dev-server": "^1.15.2"
  },
  "dependencies": {
    "webpack": "^1.13.2"
  }
}
